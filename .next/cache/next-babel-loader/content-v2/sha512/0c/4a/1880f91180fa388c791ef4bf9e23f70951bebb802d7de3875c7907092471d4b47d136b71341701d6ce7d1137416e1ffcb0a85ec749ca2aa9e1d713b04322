dSpeed = Math.round(weather.current.wind_speed);\n  const currentWindDeg = windDirection(weather.current.wind_deg);\n  const currentPressure = weather.current.pressure;\n  const currentVisibility = weather.current.visibility / 1609.34;\n  const currentUvi = Math.round(weather.current.uvi);\n  const currentConditions = weather.current.weather[0].description;\n  const currentIcon = weather.current.weather.icon;\n  const chanceOfRain = weather.hourly[0].pop * 100;\n  const precipitation = weather.minutely[0].precipitation; // const dailyDay = getDay(weather.daily[1].dt, weather.timezone_offset);\n  // const dailyIcon = weather.daily[1].weather[0].icon;\n  // const dailyMax = tempConvert(weather.daily[1].temp.max);\n  // const dailyMin = tempConvert(weather.daily[1].temp.min);\n  // function mapProductsToItems(products) {\n  //   return products.map(product => ()}\n\n  function mapDailyData(daily) {\n    return daily.map(d => (5, 8 // getDay(d.dt, weather.timezone_offset);\n    // dailyIcon = d.weather[0].icon;\n    // dailyMax = tempConvert(d.temp.max);\n    // dailyMin = tempConvert(d.temp.min);\n    ));\n  }\n\n  const dailyData = mapDailyData(weather.daily);\n  return {\n    currentSunrise,\n    currentSunset,\n    currentTemp,\n    currentFeelsLike,\n    currentHumidity,\n    currentClouds,\n    currentWindSpeed,\n    currentWindDeg,\n    currentPressure,\n    currentPressure,\n    currentVisibility,\n    currentUvi,\n    currentConditions,\n    chanceOfRain,\n    precipitation,\n    dailyData\n  };\n} // {\n//   \"lat\": 33.44,\n//   \"lon\": -94.04,\n//   \"timezone\": \"America/Chicago\",\n//   \"timezone_offset\": -21600,\n//   \"current\": {\n//     \"dt\": 1595243443,\n//     \"sunrise\": 1608124431,\n//     \"sunset\": 1608160224,\n//     \"temp\": 274.75,\n//     \"feels_like\": 270.4,\n//     \"pressure\": 1017,\n//     \"humidity\": 96,\n//     \"dew_point\": 274.18,\n//     \"uvi\": 0,\n//     \"clouds\": 90,\n//     \"visibility\": 6437,\n//     \"wind_