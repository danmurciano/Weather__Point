{"ast":null,"code":"export default function weatherResponse(weather) {\n  function timeConvert(epoch, offset) {\n    const userTime = new Date();\n    const userOffset = userTime.getTimezoneOffset() * 60;\n    const date = new Date((epoch + offset + userOffset) * 1000);\n    const optionsFull = {\n      year: 'numeric',\n      month: 'short',\n      day: 'numeric',\n      hour: 'numeric',\n      minute: 'numeric'\n    };\n    const optionsShort = {\n      year: 'numeric',\n      month: 'short',\n      day: 'numeric'\n    };\n    const options = {\n      hour: 'numeric',\n      minute: 'numeric',\n      second: 'numeric'\n    };\n    const fullDate = date.toLocaleDateString(\"en-US\", optionsFull);\n    const shortDate = date.toLocaleDateString(\"en-US\", optionsShort);\n\n    const getTime = (full, short) => full.split(short + \", \");\n\n    const time = getTime(fullDate, shortDate); // return time[1];\n\n    return date.toLocaleTimeString(\"en-US\", options);\n  }\n\n  function tempConvert(calvin) {\n    return (calvin - 273.15) * 1.8 + 32;\n  }\n\n  const currentSunrise = timeConvert(weather.current.sunrise, weather.timezone_offset);\n  const currentSunset = timeConvert(weather.current.sunset, weather.timezone_offset);\n  const currentTemp = tempConvert(weather.current.temp);\n  const currentFeelsLike = tempConvert(weather.current.feels_like);\n  const currentHumidity = weather.current.humidity;\n  const currentClouds = weather.current.clouds;\n  const currentWindSpeed = weather.current.wind_speed;\n  const currentWindDeg = weather.current.wind_deg;\n  const currentPressure = weather.current.pressure;\n  const currentVisibility = weather.current.sunset;\n  const currentUvi = weather.current.uvi;\n  const currentConditions = weather.current.weather.description;\n  const currentIcon = weather.current.weather.icon;\n  return [currentSunrise, currentSunset, currentTemp, currentFeelsLike, currentHumidity, currentClouds, currentWindSpeed, currentWindDeg, currentConditi