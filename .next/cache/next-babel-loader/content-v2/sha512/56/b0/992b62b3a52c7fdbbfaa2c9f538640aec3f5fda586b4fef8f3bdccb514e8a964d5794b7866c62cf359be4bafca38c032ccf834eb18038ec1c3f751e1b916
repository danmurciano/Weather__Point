{"ast":null,"code":"export default function weatherResponse(weather) {\n  function timeConvert(epoch, offset) {\n    const userTime = new Date();\n    const userOffset = userTime.getTimezoneOffset() * 60;\n    const date = new Date((epoch + offset + userOffset) * 1000);\n    const options = {\n      hour: 'numeric',\n      minute: 'numeric'\n    };\n    return date.toLocaleTimeString(\"en-US\", options);\n  }\n\n  function tempConvert(calvin) {\n    return Math.round((calvin - 273.15) * 1.8 + 32);\n  }\n\n  function windDirection(deg) {\n    let direction;\n    const x = deg / 22.5 + 0.5;\n\n    switch (true) {\n      case x >= 15 || x < 2:\n        direction = \"N\";\n\n      case x >= 3 && x < 6:\n        direction = \"E\";\n\n      case x >= 7 && x < 10:\n        direction = \"S\";\n\n      case x >= 11 && x < 14:\n        direction = \"W\";\n      // case (x >= 1 && x < 4):\n      // direction = direction + \"NE\";\n      // break;\n      // case (x >= 5 && x < 8):\n      // direction = direction + \"SE\";\n      // break;\n      // case (x >= 9 && x < 12):\n      // direction = direction + \"SW\";\n      // break;\n      // case (x >= 13 && x < 16):\n      // direction = direction + \"NW\";\n      // break;\n    }\n\n    return x;\n  }\n\n  const currentSunrise = timeConvert(weather.current.sunrise, weather.timezone_offset);\n  const currentSunset = timeConvert(weather.current.sunset, weather.timezone_offset);\n  const currentTemp = tempConvert(weather.current.temp);\n  const currentFeelsLike = tempConvert(weather.current.feels_like);\n  const currentHumidity = weather.current.humidity;\n  const currentClouds = weather.current.clouds;\n  const currentWindSpeed = weather.current.wind_speed; // const currentWindDeg = weather.current.wind_deg;\n\n  const currentWindDeg = windDirection(348.75);\n  const currentPressure = weather.current.pressure;\n  const currentVisibility = weather.current.sunset;\n  const currentUvi = weather.current.uvi;\n  const currentConditions = weather.current.weather.description;\n  const currentIcon = weather.current.weather.icon;\n  return {\n    currentSunrise,\n    currentSunset,\n    currentTemp,\n    currentFeelsLike,\n    currentHumidity,\n    currentClouds,\n    currentWindSpeed,\n    currentWindDeg,\n    currentPressure,\n    currentConditions\n  }; // return currentSunrise;\n} // {\n//   \"lat\": 33.44,\n//   \"lon\": -94.04,\n//   \"timezone\": \"America/Chicago\",\n//   \"timezone_offset\": -21600,\n//   \"current\": {\n//     \"dt\": 1595243443,\n//     \"sunrise\": 1608124431,\n//     \"sunset\": 1608160224,\n//     \"temp\": 274.75,\n//     \"feels_like\": 270.4,\n//     \"pressure\": 1017,\n//     \"humidity\": 96,\n//     \"dew_point\": 274.18,\n//     \"uvi\": 0,\n//     \"clouds\": 90,\n//     \"visibility\": 6437,\n//     \"wind_speed\": 3.6,\n//     \"wind_deg\": 320,\n//     \"weather\": [\n//       {\n//         \"id\": 701,\n//         \"main\": \"Mist\",\n//         \"description\": \"mist\",\n//         \"icon\": \"50n\"\n//       }\n//     ]\n//   },\n//   \"minutely\": [\n//     {\n//       \"dt\": 1595243460,\n//       \"precipitation\": 0\n//     },\n//     ...\n//   },\n//     \"hourly\": [\n//     {\n//       \"dt\": 1595242800,\n//       \"temp\": 274.75,\n//       \"feels_like\": 271.22,\n//       \"pressure\": 1017,\n//       \"humidity\": 96,\n//       \"dew_point\": 274.18,\n//       \"uvi\": 0,\n//       \"clouds\": 90,\n//       \"visibility\": 1765,\n//       \"wind_speed\": 2.43,\n//       \"wind_deg\": 303,\n//       \"weather\": [\n//         {\n//           \"id\": 804,\n//           \"main\": \"Clouds\",\n//           \"description\": \"overcast clouds\",\n//           \"icon\": \"04n\"\n//         }\n//       ],\n//       \"pop\": 0.1\n//     },\n//     ...\n//   }\n// \"daily\": [\n//         {\n//       \"dt\": 1595268000,\n//       \"sunrise\": 1608124431,\n//       \"sunset\": 1608160224,\n//       \"temp\": {\n//         \"day\": 278.14,\n//         